	<sentence id="1745350.s0" text="We studied two families with an unusual variant of neurofibromatosis (NF).">
		<entity id="1745350.s0.e0" charOffset="51-68"
			type="HP" text="neurofibromatosis" ontology_id="HP_0006746"/>
	</sentence>
	<sentence id="1745350.s1" text="The first family had spinal neurofibromas and cafe au lait spots (CLS), the second spinal neurofibromas without CLS.">
		<entity id="1745350.s1.e0" charOffset="21-41"
			type="HP" text="spinal neurofibromas" ontology_id="HP_0009735"/>
