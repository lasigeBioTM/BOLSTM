		<entity id="2466440.s2.e3" charOffset="77-89"
			type="HP" text="pointed chin" ontology_id="HP_0000307"/>
	</sentence>
	<sentence id="2466440.s3" text="Tongue thrusting is common.">
		<entity id="2466440.s3.e0" charOffset="0-16"
			type="HP" text="Tongue thrusting" ontology_id="HP_0000182"/>
	</sentence>
	<sentence id="2466440.s4" text="The movement disorder consists of a wide based, ataxic gait with frequent jerky limb movements and flapping of the hands.">
		<entity id="2366330.s3.e0" charOffset="3-20"
			type="HP" text="movement disorder" ontology_id="HP_0100022"/>
		<entity id="2466440.s4.e1" charOffset="35-58"
